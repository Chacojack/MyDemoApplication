<?xml version="1.0" encoding="utf-8"?>
<manifest
    package="demo.me.mydemoapplication"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.UIActivity"
            android:label="@string/ui"/>
        <activity
            android:name=".ui.coordinator.CoordinatorActivity"
            android:label="@string/coordinator"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity
            android:name=".ui.coordinator.CoordinatorActivity2"
            android:label="@string/coordinator"
            android:theme="@style/AppThemeNoActionBar"/>
        <activity
            android:name=".ui.coordinator.BehaviorActivity"
            android:label="@string/coordinator"
            android:theme="@style/AppTheme"/>
        <activity
            android:name=".ui.bitmap.BitmapPreviewActivity"
            android:label="@string/title_activity_bitmap_preview"/>
    </application>

</manifest>
